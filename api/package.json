{
  "name": "balcora-gate-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "refresh": "rm -rf node_modules && npm install",
    "build": "concurrently -n tsc,gulp -c blue,red \"tsc\" \"gulp\"",
    "start": "NODE_ENV=production node ./app.js",
    "start-dev": "nodemon -e ts,scss,ejs --exec npm run restart-dev",
    "restart-dev": "npm run build && NODE_ENV=development node ./app.js"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/connect-redis": "0.0.11",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express-session": "^1.15.16",
    "@types/mongoose": "^5.5.32",
    "@types/recursive-readdir": "^2.2.0",
    "@types/redis": "^2.8.14",
    "@types/uuid": "^3.4.6",
    "bcrypt": "^3.0.7",
    "concurrently": "^4.1.2",
    "connect-redis": "^4.0.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-async-errors": "^3.1.1",
    "express-resource": "^1.0.0",
    "express-session": "^1.17.0",
    "http-errors": "~1.6.3",
    "mongoose": "^5.8.0",
    "morgan": "~1.9.1",
    "recursive-readdir": "^2.2.2",
    "redis": "^2.8.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.16",
    "eslint": "^6.7.2",
    "gulp": "^4.0.2",
    "gulp-sass": "^4.0.2",
    "nodemon": "^1.19.3",
    "ts-loader": "^6.2.1",
    "tsc-watch": "^3.0.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  },
  "repository": {
    "url": "https://github.com/HW-PlayersPatch/balcora-gate-api"
  }
}
